Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"PowerContract", "[", "expr_", "]"}], ":=", 
   RowBox[{"expr", "//.", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"m_", "^", "q_"}], " ", 
        RowBox[{"n_", "^", "q_"}]}], "\[RuleDelayed]", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"m", " ", "n"}], ")"}], "^", "q"}], "/;", 
        RowBox[{
         RowBox[{"!", 
          RowBox[{"IntegerQ", "[", "m", "]"}]}], "&&", 
         RowBox[{"!", 
          RowBox[{"IntegerQ", "[", "n", "]"}]}]}]}]}], ",", 
      RowBox[{
       RowBox[{
        RowBox[{"m_", "^", "q_"}], " ", 
        RowBox[{"n_", "^", "p_"}]}], "\[RuleDelayed]", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"m", "/", "n"}], ")"}], "^", "q"}], "/;", 
        RowBox[{
         RowBox[{"q", "\[GreaterEqual]", "0"}], "&&", 
         RowBox[{"p", "\[Equal]", 
          RowBox[{"-", "q"}]}], "&&", 
         RowBox[{"!", 
          RowBox[{"IntegerQ", "[", "m", "]"}]}], "&&", 
         RowBox[{"!", 
          RowBox[{"IntegerQ", "[", "n", "]"}]}]}]}]}]}], "}"}]}]}], 
  ";"}]], "Input",
